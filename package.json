{
  "name": "iop",
  "private": true,
  "version": "0.0.10",
  "description": "a flexible framework for building web applications with elm",
  "bin": "dist/src/index.js",
  "scripts": {
    "start": "npm install && npm run dev",
    "dev": "npm run build:watch",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "publish:test": "npm run build && npm pack --dry-run",
    "local": "npm run publish:test && npm install -g ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pre63/iop.git"
  },
  "keywords": [
    "elm",
    "spa",
    "web",
    "framework"
  ],
  "author": "pre63",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pre63/iop"
  },
  "homepage": "https://github.com/pre63/iop#readme",
  "devDependencies": {
    "@types/chokidar": "2.1.3",
    "@types/mime": "2.0.3",
    "@types/node": "14.11.8",
    "@types/websocket": "1.0.1",
    "typescript": "4.0.3"
  },
  "dependencies": {
    "chokidar": "3.4.2",
    "mime": "2.4.6",
    "node-elm-compiler": "5.0.5",
    "terser": "^5.27.0",
    "websocket": "1.0.32"
  }
}
