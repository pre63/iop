  "bugs": {
    "url": "https://github.com/pre63/iop"
  },
  "homepage": "https://github.com/pre63/iop#readme",
  "devDependencies": {
    "@types/chokidar": "2.1.3",
    "@types/jest": "26.0.14",
    "@types/mime": "2.0.3",
    "@types/node": "14.11.8",
    "@types/websocket": "1.0.1",
    "jest": "26.5.3",
    "ts-jest": "26.4.1",
    "typescript": "4.0.3"
  },
  "dependencies": {
    "chokidar": "3.4.2",
    "mime": "2.4.6",
