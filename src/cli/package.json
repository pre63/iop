{
  "name": "iop",
  "version": "0.0.1",
  "description": "single page apps made easy",
  "bin": "dist/src/index.js",
  "scripts": {
    "start": "npm install && npm run dev",
    "dev": "npm run build:watch & npm run test:watch",
